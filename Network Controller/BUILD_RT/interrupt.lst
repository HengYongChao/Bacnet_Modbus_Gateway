              void ax11000_WatchDogISR(void) interrupt 12
              {
                      if (EWDI)
                      {
                              if (EWT)
                              {
                                      TA = 0xAA;
                                      TA = 0x55;
                                      WDIF = 0; // clear WatchDog Interrupt Flag.
                                      WTRF = 0; // clear the Watchdog Timer Reset Flag.
                                      RWT = 1; // reset watchdog timer.
                              }
                              else
                              {
                                      TA = 0xAA;
                                      TA = 0x55;
                                      WDIF = 0; //clear WatchDog Interrupt Flag.
                                      RWT = 1; // reset watchdog timer.
                              }
                      }
              }
              #endif
