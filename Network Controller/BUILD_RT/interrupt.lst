               * Params  : None.
               * Returns : None.
               * Note    : None.
               *--------------------------------------------------------------------------------
               */
              static void ax11000_PmmISR(void) interrupt 11 //use external interrupt 6 (0x5B)
              {
                      U8_T    wakeStatus = 0;
                      U8_T    mode = 0;
                      U8_T    sysClk = 0;
                      U8_T    canReg = 0;
                      U8_T    intEnb = 0;
              
                      EA = 0;
                      EIF &= ~INT6F;  // Clear the flag of interrupt 6.
                      EA = 1;
              
                      
              
                      wakeStatus = PCON;
                      if (!(wakeStatus & SWB_))
                      {
                              PCON &= ~ (PMM_ | STOP_);
                      }
              
              #if AX_ETH_INT_ENABLE
                      /* Ethernet wake up function */
                      /* disable wake up interrupt */
                      MDR  = 0;
